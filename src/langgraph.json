{
  "dependencies": [
    "langchain",
    "langgraph",
    "langchain-groq",
    "python-dotenv"
  ],
  "envFile": ".env",
  "graphs": {
    "main_workflow": {
      "path": "./main_workflow.py:main_workflow",
      "description": "Exoplanet research assistant workflow with conversation and exoplanet detection agent",
      "env": {
        "groq_api_key": "GROQ_API_KEY",
        "langsmith_api_key": "LANGSMITH_API_KEY"
      },
      "config_schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "groq_api_key": {
            "type": "string",
            "description": "Groq API key for LLM inference"
          },
          "langsmith_api_key": {
            "type": "string",
            "description": "Langsmith API key"
          },
          "recursion_limit": {
            "type": "integer",
            "default": 50,
            "description": "Maximum recursion limit for graph execution"
          }
        },
        "required": ["groq_api_key"]
      }
    }
  }
}
